# SMS Configuration
SMS_SIMULATION=true # Set to 'true' in development to simulate SMS without actual sending
SMS_ENABLED=true # Enable/disable SMS functionality

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000